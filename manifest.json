{
	"name": "Fix image size",
	"version": "0.0.1",
	"manifest_version": 2,
	"content_scripts" : [{
		"matches": ["<all_urls>"],
		"js" : [
			"/src/content_scripts.js"
		]
	}],
	"icons": { "16": "/img/16.png",
           "48": "/img/48.png",
          "128": "/img/128.png" },
	"permissions": [
		"<all_urls>"
	]
}
